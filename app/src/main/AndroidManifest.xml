<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ps.pdm.caderneta10" >

    <permission android:name="ps.pdm.caderneta10.model" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.ListarAlunosActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Principal"
            android:label="@string/title_activity_listar_alunos" >
        </activity>
        <activity
            android:name=".activity.CadastrarAlunoActivity"
            android:label="@string/title_activity_cadastrar_aluno" >
        </activity>
    </application>

</manifest>
